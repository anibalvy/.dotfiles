sort file | uniq      # show all lines without duplication, `sort -u` and `uniq` is the same effect.

sort file | uniq -u   # show not duplicated lines

sort file | uniq -d   # show duplicated lines only

sort file | uniq -c   # count all lines

sort file | uniq -uc  # count not duplicated lines

sort file | uniq -dc  # count only duplicated lines
